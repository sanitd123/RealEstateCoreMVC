@model RealEstateCoreMVC.Models.Account

@{
    <div>
        <form asp-controller="LoginRegister" asp-action="AddAccount" method="post">
            <div>
                Username : <input asp-for="Username" />
            </div>
            <div>
                Password : <input asp-for="Password" />
            </div>
            <div>
                Email : <input asp-for="Email" />
            </div>
            <div>
                Security Question 1 : <select asp-for="SecurityQuestion1" asp-items="@ViewData["QuestionSet1"]"></select>
            </div>
        </form>
    </div>
}
